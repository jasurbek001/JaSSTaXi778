<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CityOyna">

    <Button
        android:id="@+id/yangiqorgon_btn"
        android:layout_width="280dp"
        android:layout_height="55dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/round_radius"
        android:gravity="center"
        android:text="YANGIQO'RG'ON"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintLeft_toLeftOf="@id/passwordnum"
        app:layout_constraintRight_toRightOf="@id/passwordnum"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/chortoq_btn"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/uychi_btn"
        android:layout_width="280dp"
        android:layout_height="55dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/round_radius"
        android:gravity="center"
        android:text="UYCHI"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintLeft_toLeftOf="@id/passwordnum"
        app:layout_constraintRight_toRightOf="@id/passwordnum"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/yangiqorgon_btn"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/toraqorgon_btn"
        android:layout_width="280dp"
        android:layout_height="55dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/round_radius"
        android:gravity="center"
        android:text="TO'RAQO'RG'ON"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintLeft_toLeftOf="@id/passwordnum"
        app:layout_constraintRight_toRightOf="@id/passwordnum"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/uychi_btn"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/chortoq_btn"
        android:layout_width="280dp"
        android:layout_height="55dp"
        android:layout_marginTop="108dp"
        android:background="@drawable/round_radius"
        android:gravity="center"
        android:text="CHORTOQ"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintLeft_toLeftOf="@id/passwordnum"
        app:layout_constraintRight_toRightOf="@id/passwordnum"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view6"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/view13"
        android:layout_width="wrap_content"
        android:layout_height="100dp"
        android:background="@drawable/ic__"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="0dp" />

    <View
        android:id="@+id/view6"
        android:layout_width="407dp"
        android:layout_height="100dp"
        android:background="@drawable/ic_qale"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="-10dp" />

</androidx.constraintlayout.widget.ConstraintLayout>